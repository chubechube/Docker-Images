version: '2'
volumes:
  logcontainer: {}
services:
  FileBeat:
    labels:
      io.rancher.container.pull_image: always
      io.rancher.scheduler.affinity:host_label: "{RANCHER_ENVIRONMENT}"
    tty: true
    image: chube/filebeatpls:latest
    volumes:
    - logcontainer:/var/log/welfinity/
    stdin_open: true
  PharmacyLoadService:
    labels:
      io.rancher.container.pull_image: always
      io.rancher.scheduler.affinity:host_label: "{RANCHER_ENVIRONMENT}"
    tty: true
    image: chube/pharmacyload:latest
    volumes:
    - logcontainer:/var/log/welfinity/
    stdin_open: true
